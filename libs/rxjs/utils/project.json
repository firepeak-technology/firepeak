{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/rxjs/utils/src",
  "prefix": "firepeak",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/rxjs/utils"],
      "options": {
        "project": "libs/rxjs/utils/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/rxjs/utils/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/rxjs/utils/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/rxjs/utils"],
      "options": {
        "jestConfig": "libs/rxjs/utils/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/rxjs/utils/**/*.ts",
          "libs/rxjs/utils/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
